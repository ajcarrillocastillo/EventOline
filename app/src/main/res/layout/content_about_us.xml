<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_about_us"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.carrillo.jesus.eventoline.AboutUS"
    tools:showIn="@layout/activity_about_us">

    <TextView
        android:text="@string/Developer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textViewNombre" />

    <TextView
        android:text="JesÃºs Carrillo Castillo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewNombreHecho"
        android:layout_alignBaseline="@+id/textViewNombre"
        android:layout_alignBottom="@+id/textViewNombre"
        android:layout_toRightOf="@+id/textViewNombre"
        android:layout_toEndOf="@+id/textViewNombre"
        android:layout_marginLeft="58dp"
        android:layout_marginStart="58dp" />

    <TextView
        android:text="@string/Version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewNombre"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:id="@+id/textViewVersion" />

    <TextView
        android:text="0.9.0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewVersionEscrita"
        android:layout_alignBaseline="@+id/textViewVersion"
        android:layout_alignBottom="@+id/textViewVersion"
        android:layout_alignLeft="@+id/textViewNombreHecho"
        android:layout_alignStart="@+id/textViewNombreHecho" />

    <TextView
        android:text="@string/Licenses"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewVersion"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp"
        android:id="@+id/textViewLicencias" />

    <TextView
        android:text="Apache"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textViewLicencias"
        android:layout_alignLeft="@+id/textViewVersionEscrita"
        android:layout_alignStart="@+id/textViewVersionEscrita"
        android:id="@+id/TextViewLicencias" />

    <TextView
        android:text="@string/Number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewLicencias"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp"
        android:id="@+id/textView9" />

    <TextView
        android:text="666666666"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewTelefono"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignLeft="@+id/TextViewLicencias"
        android:layout_alignStart="@+id/TextViewLicencias" />
</RelativeLayout>
